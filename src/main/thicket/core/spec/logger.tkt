module Spec.Logger

import Data

model TestLogger {
    description : string -> unit
    success     : string -> unit
    failure     : string -> unit
    terminate   : (number,number) -> unit
}

def dumbLogger : TestLogger = {
    let whoop = _ -> () in
        TestLogger whoop whoop whoop whoop
}